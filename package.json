{
	"name": "@kudoo/components",
	"version": "2.4.2",
	"description": "Shared React components across Kudoo",
	"main": "build/index.js",
	"scripts": {
		"test": "jest --env=jsdom",
		"storybook": "cross-env ENV=storybook start-storybook -p 6006",
		"build": "cross-env ENV=storybook build-storybook",
		"copy-files": "copyfiles ./src/assets/**/* ./src/config/theme/css/fonts/**/* ./src/config/theme/css/*.scss ./src/config/theme/css/*.css ./src/**/**/*.md  ./build -u 1",
		"babel": "rm -rf build && babel ./src --extensions \".ts,.tsx,.js,.jsx\" --out-dir build && npm run copy-files",
		"babel:watch": "babel ./src --watch --extensions \".ts,.tsx,.js,.jsx\"  --out-dir build",
		"prepublishOnly": "npm run tsc:compile",
		"version": "auto-changelog -p && git add CHANGELOG.md",
		"release": "np --yolo --no-yarn",
		"tsc:watch": "ttsc --watch",
		"tsc:compile": "rm -rf build && ttsc && npm run copy-files"
	},
	"keywords": [
		"React",
		"Storybook"
	],
